{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Client",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_client",
			"program": "${workspaceFolder}/Client/bin/Debug/Client.dll",
			"args": [],
			"cwd": "${workspaceFolder}/Client/bin/Debug/",
			"console": "internalConsole",
			"logging": {
				"moduleLoad": false
			},
			"stopAtEntry": false,
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"name": "Debug Server",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build_server",
			"program": "${workspaceFolder}/Server/bin/Debug/Server.dll",
			"args": [],
			"cwd": "${workspaceFolder}/Server/bin/Debug/",
			"console": "integratedTerminal",
			"logging": {
				"moduleLoad": false
			},
			"stopAtEntry": false,
			"internalConsoleOptions": "openOnSessionStart"
		},
	]
}